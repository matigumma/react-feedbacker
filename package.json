{
  "name": "react-feedbacker",
  "version": "2.0.0",
  "description": "A minimal base for creating notifications as feedback in the UI",
  "main": "dist/index.js",
  "module": "dist/react-feedbacker.esm.js",
  "typings": "dist/index.d.ts",
  "author": "Viktor Ivarsson",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/viktorivarsson/react-feedbacker.git"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "react",
    "feedback",
    "notification",
    "notify",
    "toast"
  ],
  "scripts": {
    "test": "tsdx test --env=jsdom",
    "test:coverage": "tsdx test --env=jsdom --coverage",
    "start": "tsdx watch",
    "start:example": "cd example && npm start",
    "build": "rimraf dist && tsdx build",
    "lint": "tsdx lint src && tsc --noEmit",
    "eslint": "tsdx lint",
    "preversion": "npm test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsc --noEmit && lint-staged",
      "pre-push": "npm test"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "tsdx lint",
      "git add"
    ]
  },
  "devDependencies": {
    "@testing-library/react": "^11.2.3",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.22",
    "@types/react": "^17.0.0",
    "eslint": "^7.18.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "rimraf": "^3.0.2",
    "tsdx": "^0.14.1",
    "typescript": "^4.1.3"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "dependencies": {}
}
