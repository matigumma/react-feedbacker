{
  "name": "react-feedbacker",
  "version": "2.0.0-0",
  "description": "A minimal base for creating notifications as feedback in the UI",
  "main": "dist/index.js",
  "module": "dist/react-feedbacker.esm.js",
  "typings": "dist/index.d.ts",
  "author": "Viktor Ivarsson",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/viktorivarsson/react-feedbacker.git"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "react",
    "feedback",
    "notification",
    "notify",
    "toast"
  ],
  "scripts": {
    "test": "tsdx test --env=jsdom",
    "test:coverage": "tsdx test --env=jsdom --coverage",
    "start": "tsdx watch",
    "start:example": "cd example && npm start",
    "build": "rimraf dist && tsdx build",
    "lint": "tsdx lint src && tsc --noEmit",
    "eslint": "tsdx lint",
    "preversion": "npm test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsc --noEmit && lint-staged",
      "pre-push": "npm test"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "tsdx lint",
      "git add"
    ]
  },
  "devDependencies": {
    "@testing-library/react": "^9.1.4",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.5",
    "@types/react": "^16.9.2",
    "eslint": "^6.4.0",
    "eslint-plugin-react-hooks": "^2.0.1",
    "husky": "^3.0.5",
    "lint-staged": "^9.2.5",
    "prettier": "^1.18.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "rimraf": "^3.0.0",
    "tsdx": "^0.9.2",
    "typescript": "^3.6.3"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "dependencies": {}
}
